  "name": "react-register",
  "version": "1.2.0",
  "license": "MIT",
  "homepage": "./",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "bootstrap": "3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chartist": "0.10.1",
    "cheet.js": "^0.3.3",
    "codemirror": "^5.39.2",
    "css-loader": "^1.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "draft-js": "^0.10.5",
    "eslint": "^5.2.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "highcharts": "^6.1.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-sass": "^4.9.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "4.1.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-bootstrap": "0.32.1",
    "react-chartist": "0.13.1",
    "react-codemirror2": "^5.1.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.3.0",
    "react-google-maps": "9.4.5",
    "react-highcharts": "^16.0.2",
    "react-images": "^0.5.19",
    "react-mde": "^5.7.0",
    "react-notification-system": "0.2.17",
    "react-photo-gallery": "^6.0.28",
    "react-router-dom": "4.2.2",
    "react-skillbars": "^1.5.1",
    "showdown": "^1.8.6",
    "showdown-xss-filter": "^0.2.0",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "node scripts/start.js",
    "start-old": "npm-run-all -p watch-css start-js",
    "start": "if [ -f src/variables/SiteData.jsx ]; then npm-run-all -p watch-css start-js; else cp src/variables/SiteData.jsx-sample src/variables/SiteData.jsx && npm-run-all -p watch-css start-js; fi",
    "build": "if [ -f src/variables/SiteData.jsx ]; then npm run build-css && node scripts/build.js; else cp src/variables/SiteData.jsx-sample src/variables/SiteData.jsx && npm run build-css && node scripts/build.js; fi",
    "test": "node scripts/test.js --env=jsdom",
    "load-sample": "cp src/variables/SiteData.jsx-sample src/variables/SiteData.jsx"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
